<% include ../shared/header %>
<% contentEnitity = item;
	contentContentAttributes = (contentEnitity.contenttypeattributes && contentEnitity.contenttypeattributes.periodical)? true : false;
	contentEffect = (contentContentAttributes && contentEnitity.contenttypeattributes.periodical.effect) ? contentEnitity.contenttypeattributes.periodical.effect : false;

	contentStatic = (contentContentAttributes && contentEnitity.contenttypeattributes.periodical.static) ? contentEnitity.contenttypeattributes.periodical.static : false; %>
<body class=" ">
	<div class="periodical-page-border top"></div>
	<div class="periodical-page-border right"></div>
	<div class="periodical-page-border left"></div>
	<div class="periodical-page-border bottom"></div>
	<% if(contentEffect){ %>
		<div id="container" class="container intro-effect-<%=contentEnitity.contenttypeattributes.periodical.effect%> <%if(contentStatic){%>modify<%}%>  ">
	<%}
	else{%>
		<div id="container" class="container intro-effect-push">
	<%}%>
