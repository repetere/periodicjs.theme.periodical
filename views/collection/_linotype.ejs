<section id="linotype" class="linotype">

<% if(contentEnitity.contenttypeattributes && contentEnitity.contenttypeattributes['periodical-collection'] && contentEnitity.contenttypeattributes['periodical-collection'].useintro){
	%>

	<article class="section active" id="section0">

			<% contentBgImgUseDefault = false;
				  contentTitleLink = false;
				  %>

				<% include ../shared/partial/_contentEntity_bgImg_background %>
				<% include ../shared/partial/_contentEntityTitle_subTitle %>
		  </article>
	<%
	}%>
	<% if(collection.items && collection.items.length>0){
		collection.items.forEach(function(itemObj,index){%>
			<article class="section " id="section<%- (index+1) %>">
				<% var contentEnitity = itemObj.item;
					  contentContentAttributes = (contentEnitity.contenttypeattributes && contentEnitity.contenttypeattributes.periodical)? true : false; 
					  contentBgImgUseDefault = false;
				  contentTitleLink = true;
				  contentDimBigImg = true;
					  %>

				  
				<% include ../shared/partial/_contentEntity_bgImg_background %>
				<% include ../shared/partial/_contentEntityTitle_subTitle %>
		  </article>
		<%});
	}%>
</section>