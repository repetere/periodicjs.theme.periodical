<article class="content">
	<div>
		<%- contentEnitity.content %>
	</div>
</article>
	<% if(contentEnitity.tags && contentEnitity.tags.length >0){
		%> <div class="tags">
		<small>
		<strong>Tags: </strong> <%
			for(var x = 0; x < contentEnitity.tags.length; x++){
				%>
				<a href="/tag/<%- contentEnitity.tags[x].name %>">#<%- contentEnitity.tags[x].title %></a><% if(x !== contentEnitity.tags.length-1){%>,<%}%> 
				<%
			} %>
			</small>
			</div><%
		} %>	